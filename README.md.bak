| im      | implements
| ie      | interface
| tw      | throw _new IllegalStateException()_;
| ir      | import
| le      | length
| sout    | System.out.println(" ");
| soutv   | System.out.println(_"args = " + args_);
| wh      | while (_true_) {  }
| vo      | volatile
| iof     | instanceof
